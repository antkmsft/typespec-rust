{
  "name": "@azure-tools/typespec-rust",
  "version": "0.28.0",
  "description": "TypeSpec emitter for Rust SDKs",
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "tspMain": "dist/src/index.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "spector": "node .scripts/spector.js",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0",
    "build": "tsc -p .",
    "test": "vitest run",
    "test-ci": "vitest run --coverage --reporter=junit --reporter=default --no-file-parallelism",
    "tspcompile": "node .scripts/tspcompile.js",
    "watch": "tsc -p . --watch",
    "workspace-members": "node .scripts/workspace-members.js"
  },
  "files": [
    "dist/src/**",
    "!dist/test/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/typespec-rust.git"
  },
  "keywords": [
    "typespec",
    "rust",
    "rustlang",
    "typespec"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/typespec-rust/issues"
  },
  "homepage": "https://github.com/Azure/typespec-rust#readme",
  "readme": "https://github.com/Azure/typespec-rust/blob/main/readme.md",
  "devDependencies": {
    "@azure-tools/azure-http-specs": "0.1.0-alpha.31",
    "@azure-tools/typespec-autorest": "~0.61.0",
    "@azure-tools/typespec-azure-core": "~0.61.0",
    "@azure-tools/typespec-azure-resource-manager": "~0.61.0",
    "@azure-tools/typespec-client-generator-core": "~0.61.2",
    "@eslint/js": "^9.35.0",
    "@types/node": "^20.19.16",
    "@typespec/compiler": "^1.4.0",
    "@typespec/http": "^1.4.0",
    "@typespec/http-specs": "0.1.0-alpha.27",
    "@typespec/openapi": "^1.4.0",
    "@typespec/rest": "~0.75.0",
    "@typespec/spector": "0.1.0-alpha.19",
    "@typespec/versioning": "~0.75.0",
    "@typespec/xml": "~0.75.0",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "c8": "^10.1.3",
    "eslint": "^9.35.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^4.0.4"
  },
  "peerDependencies": {
    "@azure-tools/typespec-client-generator-core": ">=0.61.2 <1.0.0",
    "@typespec/compiler": "^1.5.0",
    "@typespec/http": "^1.5.0"
  },
  "dependencies": {
    "@azure-tools/codegen": "~2.10.1",
    "@azure-tools/linq": "~3.1.263",
    "@types/turndown": "^5.0.5",
    "linkifyjs": "^4.3.2",
    "query-string": "9.3.1",
    "source-map-support": "0.5.21",
    "turndown": "^7.2.1"
  },
  "azure-sdk/emitter-package-json-pinning": [
    "@azure-tools/typespec-azure-core",
    "@azure-tools/typespec-client-generator-core",
    "@typespec/compiler",
    "@typespec/http",
    "@typespec/openapi",
    "@typespec/rest",
    "@typespec/versioning",
    "@typespec/xml"
  ]
}