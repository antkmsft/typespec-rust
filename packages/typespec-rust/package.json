{
  "name": "@azure-tools/typespec-rust",
  "version": "0.8.2",
  "description": "TypeSpec emitter for Rust SDKs",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "tspMain": "dist/src/index.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "spector": "node .scripts/spector.js",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0",
    "build": "tsc -p .",
    "test": "vitest run",
    "test-ci": "vitest run --coverage --reporter=junit --reporter=default --no-file-parallelism",
    "tspcompile": "node .scripts/tspcompile.js",
    "watch": "tsc -p . --watch",
    "workspace-members": "node .scripts/workspace-members.js"
  },
  "files": [
    "dist/src/**",
    "!dist/test/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/typespec-rust.git"
  },
  "keywords": [
    "typespec",
    "rust",
    "rustlang",
    "typespec"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/typespec-rust/issues"
  },
  "homepage": "https://github.com/Azure/typespec-rust#readme",
  "readme": "https://github.com/Azure/typespec-rust/blob/main/readme.md",
  "devDependencies": {
    "@azure-tools/azure-http-specs": "0.1.0-alpha.5",
    "@azure-tools/typespec-autorest": "0.50.0",
    "@azure-tools/typespec-azure-core": "0.50.0",
    "@azure-tools/typespec-azure-resource-manager": "0.50.0",
    "@azure-tools/typespec-client-generator-core": "0.50.0",
    "@eslint/js": "^9.19.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.16",
    "@typespec/compiler": "0.64.0",
    "@typespec/http": "0.64.0",
    "@typespec/http-specs": "0.1.0-alpha.7",
    "@typespec/openapi": "0.64.0",
    "@typespec/rest": "0.64.0",
    "@typespec/spector": "0.1.0-alpha.6",
    "@typespec/versioning": "0.64.0",
    "@typespec/xml": "0.64.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "c8": "^9.1.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "peerDependencies": {
    "@azure-tools/typespec-client-generator-core": ">=0.50.0 <1.0.0",
    "@typespec/compiler": ">=0.64.0 <1.0.0"
  },
  "dependencies": {
    "@azure-tools/codegen": "~2.9.2",
    "@azure-tools/linq": "~3.1.263",
    "js-yaml": "^4.1.0",
    "query-string": "9.1.1",
    "source-map-support": "0.5.21",
    "vitest": "^1.6.1"
  }
}
