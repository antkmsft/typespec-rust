{
  "name": "@azure-tools/typespec-rust",
  "version": "0.3.0",
  "description": "TypeSpec emitter for Rust SDKs",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "tspMain": "dist/src/index.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "cadl-ranch": "node .scripts/cadl-ranch.js",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0",
    "build": "tsc -p .",
    "test": "vitest run",
    "test-ci": "vitest run --coverage --reporter=junit --reporter=default --no-file-parallelism",
    "tspcompile": "node .scripts/tspcompile.js",
    "watch": "tsc -p . --watch"
  },
  "files": [
    "dist/src/**",
    "!dist/test/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/typespec-rust.git"
  },
  "keywords": [
    "cadl",
    "rust",
    "rustlang",
    "typespec"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/typespec-rust/issues"
  },
  "homepage": "https://github.com/Azure/typespec-rust#readme",
  "readme": "https://github.com/Azure/typespec-rust/blob/main/readme.md",
  "devDependencies": {
    "@azure-tools/cadl-ranch": "0.16.1",
    "@azure-tools/cadl-ranch-expect": "0.15.6",
    "@azure-tools/cadl-ranch-specs": "0.39.2",
    "@azure-tools/typespec-autorest": "0.48.0",
    "@azure-tools/typespec-azure-core": "0.48.0",
    "@azure-tools/typespec-azure-resource-manager": "0.48.0",
    "@azure-tools/typespec-client-generator-core": "0.48.1",
    "@types/js-yaml": "~4.0.9",
    "@types/node": "^20.14.10",
    "@typespec/compiler": "0.62.0",
    "@typespec/http": "0.62.0",
    "@typespec/openapi": "0.62.0",
    "@typespec/rest": "0.62.0",
    "@typespec/versioning": "0.62.0",
    "@typespec/xml": "0.62.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "c8": "^9.1.0",
    "eslint": "~9.16.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "~8.17.0"
  },
  "peerDependencies": {
    "@azure-tools/typespec-client-generator-core": ">=0.48.1 <1.0.0",
    "@typespec/compiler": ">=0.62.0 <1.0.0"
  },
  "dependencies": {
    "@azure-tools/codegen": "~2.9.2",
    "@azure-tools/linq": "~3.1.263",
    "js-yaml": "~4.1.0",
    "query-string": "9.1.1",
    "source-map-support": "0.5.21",
    "vitest": "^1.6.0"
  }
}
